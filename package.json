  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\" \"config/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "prestart:prod": "npm run typeorm:prod:migration:run",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --nolazy --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm:prod:migration:generate": "typeorm-ts-node-commonjs -d ./ormconfig.js migration:generate",
    "typeorm:prod:migration:run": "typeorm-ts-node-commonjs -d ./ormconfig.js migration:run",
    "typeorm:prod:migration:revert": "typeorm-ts-node-commonjs -d ./ormconfig.js migration:revert",
    "typeorm:prod:schema:sync": "typeorm-ts-node-commonjs -d ./ormconfig.js schema:sync"
  },
